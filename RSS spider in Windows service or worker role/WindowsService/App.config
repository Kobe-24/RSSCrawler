<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1"/>
    </startup>
  <appSettings>
    <add key="RSSFeeds" value="http://feeds.feedburner.com/QuantitativeTrading/;https://quantivity.wordpress.com/feed/;http://quantpedia.com/Blog/Feed;http://feeds.feedburner.com/Quantocracy;https://www.quantstart.com/feed;http://gekkoquant.com/feed/" />
    <add key="RefreshIntervalInMinutes" value="5" />
    <add key="Redis.ConnectionString" value="127.0.0.1:6379" />    
    <add key="Redis.KeyPrefix" value="FeedAggregator:" />
    <add key="Redis.CacheExpirationInMinutes" value="1440" />
    <add key="Redis.ExpirationIntervalInMinutes" value="1" />
    <add key="Redis.BaseScore" value="1000" />
  </appSettings>

